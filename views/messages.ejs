<ul>
        <% for(let i = 0; i < messages.length; i++){ %>
            <li>
                <%= messages[i].user %>
                <%= messages[i].text %>
                <%= messages[i].added.toLocaleString() %>
                 <a href="/message/<%= messages[i].id %>">Open</a>
            </li>
        <% } %>
</ul>